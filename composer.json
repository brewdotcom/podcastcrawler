{
    "name": "brewdotcom/podcastcrawler",
    "version": "1.1.2",
    "type": "library",
    "description": "PHP library to find podcasts",
    "keywords": ["framework", "rest", "api", "itunes", "digitalpodcast", "provider", "mp3", "podcast", "library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dorian Neto",
            "email": "doriansampaioneto@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/podcastcrawler/podcastcrawler/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brewdotcom/podcastcrawler"
        }
    ],
    "require": {
        "podcastcrawler/podcastcrawler": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "BrewDotCom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrewDotCom\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
